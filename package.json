{
  "name": "dicebox",
  "version": "0.1.0",
  "description": "Dice rolling library that mimics roll20 as much as possible",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-dts/index.d.ts",
  "scripts": {
    "build:ne": "nearleyc src/parser/grammar.ne -o src/parser/grammar.ts",
    "build:esm": "tsc -p ./tsconfig/tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig/tsconfig.cjs.json",
    "build:dts": "tsc -p ./tsconfig/tsconfig.dts.json",
    "build": "npm run build:ne && npm run build:esm && npm run build:cjs && npm run build:dts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "dice",
    "random",
    "rpg",
    "roll20"
  ],
  "author": "MrJohz <jonathan.frere@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@types/nearley": "^2.11.0",
    "nearley": "^2.13.0",
    "tslib": "^1.9.2"
  }
}
